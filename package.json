{
  "name": "parble",
  "version": "1.0.0",
  "description": "Parble Javascript SDK",
  "main": "./lib/cjs/index.js",
  "types": "./lib/cjs/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "build:clean": "npm run clean && npm run build",
    "prepack": "npm run build:clean",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parblelabs/parble-javascript.git"
  },
  "keywords": [
    "parble",
    "parblelabs",
    "document processing",
    "ai",
    "ocr"
  ],
  "author": "parblelabs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parblelabs/parble-javascript/issues"
  },
  "homepage": "https://github.com/parblelabs/parble-javascript#readme",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "form-data": "^4.0.0"
  }
}